<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>Log</filter-name>
        <filter-class>SaldanaEnriquezCarlosAbraham.Log</filter-class>
    </filter>
    <filter>
        <filter-name>MideTiempo</filter-name>
        <filter-class>SaldanaEnriquezCarlosAbraham.MideTiempo</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>MideTiempo</filter-name>
        <servlet-name>calculadora</servlet-name>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Log</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>calculadora</servlet-name>
        <servlet-class>SaldanaEnriquezCarlosAbraham.calculadora</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>holaservlet</servlet-name>
        <servlet-class>SaldanaEnriquezCarlosAbraham.holaservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>calculadora</servlet-name>
        <url-pattern>/calculadora</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>holaservlet</servlet-name>
        <url-pattern>/holaservlet</url-pattern>
    </servlet-mapping>
</web-app>
